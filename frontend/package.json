{
  "name": "rabistyle",
  "version": "0.1.0",
  "description": "Rabi && PL Database Client",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "cross-env ./node_modules/.bin/jest",
    "typed": "cross-env ./node_modules/.bin/flow",
    "lint": "cross-env ./node_modules/.bin/eslint src",
    "check:all": "yarn test && yarn typed && yarn lint",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "build:builder": "rm *webpack.config.js -f && cross-env ./node_modules/.bin/webpack builder/make.js --output-filename=webpack.config.js --output-library=webpackConfig --output-library-target=commonjs2 --target=node --module-bind=js=babel-loader --plugin=./builder/node-externals-plugin.js",
    "build:dll:vendor": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --env.task=dll --env.dll=vendor",
    "build:dll:hmr": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --env.task=dll --env.dll=hmr",
    "build:dll": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --env.task=dll",
    "build:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot -d --env.task=app --env.env=development",
    "build:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --env.task=app --env.env=production",
    "start:init": "yarn build:builder && yarn build:dll && yarn start",
    "start": "yarn build:dev",
    "build": "yarn build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuffiy/Rabi.PL.DB.git"
  },
  "keywords": [
    "rabi",
    "pl",
    "database",
    "mysql"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuffiy/Rabi.PL.DB/issues"
  },
  "homepage": "https://github.com/yuffiy/Rabi.PL.DB#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cache-loader": "^1.0.3",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.45.0",
    "getstorybook": "^1.7.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "https://github.com/yuffiy/html-webpack-plugin.git",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.1",
    "nock": "^9.0.13",
    "postcss-color-function": "^4.0.0",
    "postcss-easings": "^1.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nesting": "^4.0.1",
    "postcss-short": "^3.0.3",
    "postcss-strip-inline-comments": "^0.1.5",
    "prettier": "^1.3.1",
    "react-hot-loader": "next",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.17.0",
    "thread-loader": "^1.0.1",
    "webpack": "^2.5.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-node-externals": "^1.5.4",
    "worker-loader": "^0.8.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^lib/(.*)$": "<rootDir>/src/lib/$1",
      "^style/(.*)$": "<rootDir>/src/lib/styles/$1",
      "^view/(.*)$": "<rootDir>/src/view/$1",
      "^component/(.*)$": "<rootDir>/src/component/$1",
      "^core/(.*)$": "<rootDir>/src/core/$1",
      "\\.css$": "identity-obj-proxy",
      "\\.(webp|jpg|png|svg)$": "<rootDir>/builder/mocks/fileTransformer.js"
    }
  },
  "dependencies": {
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote --parser=flow",
      "git add"
    ],
    "gitDir": "../"
  }
}
